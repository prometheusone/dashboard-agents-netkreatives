{
  "name": "mission-control-dashboard",
  "version": "1.0.0",
  "description": "Unified Mission Control Dashboard - Tasks, Chat, Agent Status",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/server/index.ts",
    "dev:client": "vite",
    "build": "vite build && bun build src/server/index.ts --target bun --outdir dist/server",
    "start": "NODE_ENV=production bun run dist/server/index.js",
    "preview": "vite preview"
  },
  "keywords": ["mission-control", "dashboard", "agents"],
  "author": "Netkreatives",
  "license": "UNLICENSED",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.90.20",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "hono": "^4.11.7",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "ws": "^8.18.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^5.1.3",
    "bun-types": "latest",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
